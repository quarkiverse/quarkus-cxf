            <dependency>
                <groupId>org.ehcache</groupId>
                <artifactId>ehcache</artifactId>
                <version>${ehcache.version}</version>
                <classifier>jakarta</classifier>
            </dependency>
            <dependency><!-- org.glassfish.jaxb:jaxb-runtime is managed in io.quarkus-quarkus-bom
                  so we'd normally would not need to manage it also here.
                  It is here because ehcache declares a version interval dependency on jaxb-runtime
                  which makes the BOM flattener resolve all available versions of jaxb-runtime.
                  Some of those (or their transitives) reside in Maven repos that do not exist anymore
                  and thus BOM flattener would fail without having jaxb-runtime version fixed here. -->
                <groupId>org.glassfish.jaxb</groupId>
                <artifactId>jaxb-runtime</artifactId>
                <version>${glassfish.jaxb.version}</version>
            </dependency>
            <dependency><!-- org.apache.cxf:cxf-tools-wsdlto-core declares an older version  -->
                <groupId>org.glassfish.jaxb</groupId>
                <artifactId>jaxb-xjc</artifactId>
                <version>${glassfish.jaxb.version}</version>
            </dependency>
            <dependency>
                <groupId>org.jvnet.mimepull</groupId>
                <artifactId>mimepull</artifactId>
                <version>${mimepull.version}</version>
            </dependency>
