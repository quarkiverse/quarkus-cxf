{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":dependencyDashboard",
    ":rebaseStalePrs",
    ":enableVulnerabilityAlertsWithLabel('security')"
  ],
  "ignorePresets": [
    ":prHourlyLimit2"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "io.quarkiverse.cxf", "org.glassfish.jaxb:jaxb-runtime"
      ],
      "enabled": false
    }
  ]
}